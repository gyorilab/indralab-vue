{
  "name": "indralab-vue",
  "version": "0.1.1",
  "private": false,
  "author": "Patrick Greene <patrick.anton.greene@gmail.com>",
  "repository": "indralab/indralab-vue",
  "main": "dist/IndralabVue.common.js",
  "unpkg": "dist/IndralabVue.umd.min.js",
  "jsdelivr": "dist/IndralabVue.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name IndralabVue src/index.js",
    "lint": "vue-cli-service lint",
    "watch": "npm run build -- --watch",
    "test": "npm run watch -- --mode development",
    "deploy": "python deploy"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.4",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-service": "^4.5.11",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-plugin-vue": "^5.0.0",
    "minimist": ">=1.2.3",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
